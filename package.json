{
  "name": "lit-icon",
  "version": "1.0.1",
  "description": "LitIcon is a simple Web Component to display an icon from a set of SVG icons. Inspired by Polymer iron-icon.",
  "keywords": [
    "webcomponents",
    "lit-element",
    "icons",
    "iron-icon",
    "svg"
  ],
  "main": "pkg/dist-src/index.js",
  "module": "pkg/dist-src/index.js",
  "dependencies": {},
  "devDependencies": {
    "@open-wc/testing": "^2.5.16",
    "@open-wc/testing-karma": "^4.0.9",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-standard-pkg": "^0.7.1",
    "deepmerge": "^4.2.2",
    "karma": "^6.4.1"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ]
    ]
  },
  "scripts": {
    "build": "npx pika build",
    "test": "npx karma start",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syu93/lit-icon.git"
  },
  "author": "syu93",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/syu93/lit-icon/issues"
  },
  "homepage": "https://github.com/syu93/lit-icon#readme"
}
